#!/bin/sh

for f in data/grib/*; do
    name=$(basename $f)
    resultfile=results/gribtoqd/$name
    tmpfile=${resultfile}.tmp
    ../gribtoqd -o $tmpfile $f
    cmp --quiet $resultfile $tmpfile
    printf '%-60s' "$name"
    if [[ $? -eq 0 ]]; then
	echo OK
	rm -f $tmpfile
    else
	echo FAILED
    fi
done
